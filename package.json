{
  "name": "mailto",
  "version": "1.0.0",
  "description": "replaces obscured elememt with a mailto link",
  "main": "index.js",
  "author": "Ted Whitehead <hello@tedwhitehead.com>",
  "contributors": ["Mat Brady <matbr8dy@gmail.com>"],
  "repository": {
    "type": "git",
    "url": "git://github.com/Threespot/mailto"
  },
  "keywords": ["mailto"],
  "bugs": {
    "url": "https://github.com/Threespot/mailto/issues"
  },
  "engines": {
    "node": ">=6.0.0",
    "npm": ">=3.0.0"
  },
  "license": "MIT",
  "private": true,
  "scripts": {
    "build":
      "microbundle --output dist --compress false && microbundle --output dist/compressed --compress false",
    "dev": "microbundle watch --output dist --compress false",
    "docs":
      "jsdoc index.js -t node_modules/minami --readme README.md --destination docs",
    "test":
      "jest --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "devDependencies": {
    "coveralls": "^3.0.0",
    "jest": "^22.0.4",
    "jsdoc": "^3.5.5",
    "microbundle": "^0.2.4",
    "minami": "^1.2.3",
    "pre-commit": "^1.2.2"
  }
}
